@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<http://github.com/danilobellini/lz-plugins-lv2/robotize>
  a lv2:Plugin, lv2:SpectralPlugin;

  doap:name "Robotize";

  doap:developer [
    foaf:name "Danilo de Jesus da Silva Bellini";
    foaf:homepage <http://github.com/danilobellini>;
    foaf:mbox <mailto:danilo.bellini@gmail.com>;
  ];

  doap:maintainer [
    foaf:name "Danilo de Jesus da Silva Bellini";
    foaf:homepage <http://github.com/danilobellini>;
    foaf:mbox <mailto:danilo.bellini@gmail.com>;
  ];

  doap:license <GPLv3>;

  rdfs:comment """
  Simple voice robotization plugin using the AudioLazy Python DSP.
  """;

  lv2:port
  [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "In";
    lv2:name "In";
  ],
  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "Out";
    lv2:name "Out";
  ];

  lv2:binary <lz-robotize.so>.
